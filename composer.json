{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "dg/rss-php": "^1.5",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "nunomaduro/collision": "^5.6",
        "spatie/browsershot": "^3.48",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/enum": "^3.9",
        "spatie/fork": "^1.1",
        "spatie/laravel-enum": "^2.5",
        "spatie/laravel-feed": "^4.0",
        "spatie/laravel-flash": "^1.8",
      "ext-simplexml": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracasts/cypress": "^3.0",
        "laravel/dusk": "^6.15",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.4",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ray": "^1.19",
        "spatie/laravel-stubs": "^2.1",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "spatie/phpunit-watcher": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files" : [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan spatie-stub:publish"
        ],
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "setup": [
            "@php artisan migrate:fresh --seed",
            "@composer ide-helper"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
